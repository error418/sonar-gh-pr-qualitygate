{
  "name": "swingletree",
  "description": "Enforce SonarQube quality gate checks in your GitHub pull requests",
  "repository": {
    "type": "git",
    "url": "git@github.com:error418/swingletree.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "npm run build && node bin/main.js",
    "build": "npm run build-ts && npm run tslint",
    "test": "ts-mocha --exit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "nodemon --inspect bin/main.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "@octokit/rest": "^16.25.2",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "express-github-webhook": "^1.0.6",
    "inversify": "^5.0.1",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.10.0",
    "nunjucks": "^3.2.0",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.6",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.0.0",
    "@types/nunjucks": "^3.1.1",
    "@types/redis": "^2.8.12",
    "@types/request": "^2.48.1",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "mockserver": "^3.0.0",
    "nodemon": "^1.19.0",
    "semantic-release": "^15.13.12",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-mocha": "^6.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "license": "GPL-3.0",
  "private": true
}
