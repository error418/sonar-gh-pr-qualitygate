{
  "name": "swingletree",
  "description": "Enforce SonarQube quality gate checks in your GitHub pull requests",
  "repository": {
    "type": "git",
    "url": "git@github.com:error418/swingletree.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "npm run build && nodemon bin/main.js",
    "build": "npm run build-ts && npm run tslint",
    "test": "ts-mocha",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "nodemon --inspect bin/main.js",
    "release": "semantic-release"
  },
  "dependencies": {
    "@octokit/rest": "^15.11.2",
    "@types/basic-auth": "^1.1.2",
    "@types/js-yaml": "^3.11.1",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/redis": "^2.8.6",
    "@types/request": "^2.47.1",
    "basic-auth": "^2.0.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "express-github-webhook": "^1.0.6",
    "inversify": "^4.13.0",
    "js-yaml": "^3.11.0",
    "jsonwebtoken": "^8.2.1",
    "pug": "^2.0.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/compression": "0.0.34",
    "@types/express": "^4.11.1",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.10",
    "@types/sinon": "^2.3.7",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.3",
    "semantic-release": "^15.5.4",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.14.0",
    "ts-mocha": "^1.2.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "license": "GPL-3.0",
  "private": true
}
